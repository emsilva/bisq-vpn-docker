# VPN Configuration
VPN_SERVICE_PROVIDER=custom
VPN_TYPE=wireguard

# WireGuard specific configuration
WIREGUARD_PRIVATE_KEY=YOUR_PRIVATE_KEY_HERE
WIREGUARD_ADDRESSES=10.x.x.x/32
WIREGUARD_PUBLIC_KEY=YOUR_PUBLIC_KEY_HERE
WIREGUARD_PRESHARED_KEY=YOUR_PRESHARED_KEY_HERE
WIREGUARD_ENDPOINT_IP=x.x.x.x
WIREGUARD_ENDPOINT_PORT=1234
WIREGUARD_PERSISTENT_KEEPALIVE_INTERVAL=15s
WIREGUARD_MTU=1320
WIREGUARD_ALLOWED_IPS=0.0.0.0/0

# Kill switch - blocks all traffic if VPN dies
KILL_SWITCH=on

# DNS Configuration
DNS_KEEP_NAMESERVER=off
DOT=on
DOT_PROVIDERS=cloudflare
BLOCK_MALICIOUS=on
BLOCK_ADS=off

# Firewall Configuration
FIREWALL_INPUT_PORTS=6080,5901

# Logging
LOG_LEVEL=info

# Health check
HEALTH_SERVER_ADDRESS=:8000
HEALTH_VPN_DURATION_INITIAL=30s
HEALTH_VPN_DURATION_ADDITION=5s
HTTP_CONTROL_SERVER_AUTH=off

# Timezone
TZ=America/Sao_Paulo